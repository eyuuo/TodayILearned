<Baekjoon Practice>
1. https://www.acmicpc.net/problem/10809 (C)

#include <stdio.h>
#include <string.h>

int main()
{
   char str[101];
   int alphabet[26];
   int temp;
    
   // initialize alphabet[]
   for(int i = 0; i < 26; i ++) {
   		alphabet[i] = -1;
   }
   
   scanf("%s", str);
    
   for(int i = 0; i < strlen(str); i ++) {
       // temp represents alphabetical order of str[i]
       temp = str[i] - 'a';
        
       if(alphabet[temp] == -1)
           alphabet[temp] = i;
   }
    
   for(int i = 0; i < 26; i ++) {
    	printf("%d ", alphabet[i]);
   }
   
   return 0;
}

2. https://www.acmicpc.net/problem/2675 (JAVA)

import java.util.Scanner;
import java.lang.*;

class Main {
	public static void main(String[] args) {
		int test, len;

		Scanner kb = new Scanner (System.in);
		
		test = kb.nextInt();
		
		String[] str = new String[test];
		int[] repetition = new int[test];

		// test case의 갯수만큼 반복하여 repetition, str 입력받기
		for(int i = 0; i < test; i ++) {
			str[i] = kb.next();
			repetition[i] = Integer.parseInt(str[i]);

			str[i] = kb.next();
		}
		
		for(int i = 0; i < test; i ++) {
			len = str[i].length();
			
			for(int j = 0; j < len; j ++) {
				for(int k = 0; k < repetition[i]; k ++)
						System.out.print(str[i].charAt(j));
			}
					System.out.println();
		}
		
		kb.close();
	}
}
